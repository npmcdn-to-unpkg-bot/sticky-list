{
  "name": "sticky-list",
  "version": "1.0.3",
  "description": "A javascript timeline module that features a sticky index.",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "babel modules --out-dir lib",
    "build:umd:min": "NODE_ENV=production webpack modules/index.js dist/sticky-list.min.js --config webpack.config.production.js",
    "build:umd": "webpack modules/index.js dist/sticky-list.js --config webpack.config.development.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint modules examples",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm test",
    "start": "node examples/server.js",
    "test": "npm run lint",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rnolan3/sticky-list.git"
  },
  "author": "Robert Nolan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rnolan3/sticky-list/issues"
  },
  "homepage": "https://github.com/rnolan3/sticky-list#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.21",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "capitalize": "^1.0.0",
    "css-loader": "^0.23.1",
    "ejs": "^2.3.4",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.14.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "gzip-size": "^3.0.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "pretty-bytes": "^3.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0"
  }
}
